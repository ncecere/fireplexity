# Firecrawl configuration
# FIRECRAWL_BASE_URL defaults to https://api.firecrawl.dev if not set
FIRECRAWL_API_KEY=fc-YOUR_FIRECRAWL_API_KEY
FIRECRAWL_BASE_URL=https://api.firecrawl.dev

# OpenAI (or compatible) configuration
# OPENAI_BASE_URL defaults to https://api.openai.com/v1 if not set
# OPENAI_MODEL defaults to gpt-4o-mini if not set
# OPENAI_API_MODE can be responses (default), chat, or completions for OpenAI-compatible proxies
OPENAI_API_KEY=sk-YOUR_OPENAI_API_KEY
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4o-mini
OPENAI_API_MODE=responses
